import java.util.*;
public class sumofelements{
  public static void main(String args[]) {
    int arr[] = {1,2,3,4,5,10,20};

     System.out.println("The sum of the elements of the array is "+sum(arr));
  }


static int sum(int arr[]){

    int sum = 0;
    for (int i = 0; i < arr.length; i++) {
      sum += arr[i];
    }
   return sum;
  }
  
}
